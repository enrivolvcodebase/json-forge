{
  "name": "@enrivolv/json-forge",
  "version": "1.1.0",
  "description": "A powerful JSON parser and TypeScript/JavaScript model generator. Parse JSON effortlessly and automatically generate type-safe models for your projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "json",
    "parser",
    "typescript",
    "javascript",
    "model-generator",
    "code-generation",
    "type-safe",
    "json-to-model",
    "schema",
    "enrivol"
  ],
  "author": "Enrivolv IT Solutions Pvt Ltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enrivolvcodebase/json-forge"
  },
  "bugs": {
    "url": "https://github.com/enrivolvcodebase/json-forge/issues"
  },
  "homepage": "https://github.com/enrivolvcodebase/json-forge#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
